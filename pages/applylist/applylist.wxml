<!--pages/applylist/applylist.wxml-->
<view class='index'>
  <view class="index-bd">
	    <view class="kind-list">
	      <block wx:for-items="{{applylist}}" wx:key="{{applylist.id}}">
	        <view class="kind-list-item">
              <view class="kind-list-item-hd}" >
                  <text class="kind-list-text">{{applylist[index].applyContent}}</text>
                  <text class="kind-list-right-text-nor" >{{applylist[index].state}}</text>
                </view>
          </view>
	      </block>
	    </view>
	  </view>
</view>
